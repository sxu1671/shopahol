{
  "manifest_version": 2,
  "name": "Shopahol",
  "description": "To track what shopaholics are addicted to",
  "version": "0.7",
  "author": "Sophia Xu, Jacky Zhu",
  
  "permissions": [
    "webRequest",
    "*://*.amazon.com/*"
  ],
  "content_scripts": [
  {
    "matches": [
      "*://*.amazon.com/*"
    ],
    "js": ["content.js", "util.js"]
  }
],
"background": {
  "scripts": [ "bg.js"]
},
"browser_action": {}

}